<%- include('../partials/header'); -%>
  <style>
    body {
      font-family: Arial, sans-serif;
      background-color: #f5f5f5;
    }

    .question-answer {
      background-color: #fff;
      box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
      border-radius: 5px;
      padding: 20px;
      margin-bottom: 20px;
    }

    .question {
      font-size: 18px;
      font-weight: bold;
      margin-bottom: 10px;
      color: #333;
    }

    .answer {
      font-size: 16px;
      color: #666;
      margin-left: 20px;
    }
  </style>
</head>
<body>

  <% questions.forEach((question, index) => { %>
    <div class="question-answer">
      <div class="question"><%= index + 1 %>. <%= question.question %> <%= question.maxScore%></div>
      <div class="answer"><%= question.answer %></div>
    </div>
  <% }); %>

<%- include('../partials/footer'); -%>
